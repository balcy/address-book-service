<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="com.canonical.galera.AddressBook">
    <signal name="contactsUpdated">
      <arg name="ids" type="as" direction="out"/>
    </signal>
    <signal name="contactsRemoved">
      <arg name="ids" type="as" direction="out"/>
    </signal>
    <signal name="contactsCreated">
      <arg name="ids" type="as" direction="out"/>
    </signal>

    <method name="availableSources">
        <arg type="o" direction="out"/>
    </method>
    <method name="sortFields">
      <arg type="as" direction="out"/>
    </method>
    <method name="query">
      <arg name="clause" type="s" direction="in"/>
      <arg name="sort" type="s" direction="in"/>
      <arg name="sources" type="as" direction="in"/>
      <arg type="o" direction="out"/>
    </method>
    <method name="removeContacts">
      <arg type="b" direction="out"/>
      <arg name="contactIds" type="as" direction="in"/>
    </method>
    <method name="createContact">
      <arg name="contact" type="o" direction="in"/>
      <arg name="source" type="s" direction="in"/>
      <arg type="s" direction="out"/>
    </method>
    <method name="updateContact">
      <arg name="contact" type="o" direction="in"/>
      <arg type="b" direction="out"/>
    </method>
    <method name="linkContacts">
      <arg type="s" direction="out"/>
      <arg name="contacts" type="as" direction="in"/>
    </method>
    <method name="unlinkContacts">
      <arg type="b" direction="out"/>
      <arg name="parent" type="s" direction="in"/>
      <arg name="contacts" type="as" direction="in"/>
    </method>
  </interface>
</node>
