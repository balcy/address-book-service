<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="com.canonical.galera.View">
    <signal name="contactsUpdated">
      <arg name="pos" type="i" direction="out"/>
      <arg name="lenght" type="i" direction="out"/>
    </signal>
    <signal name="contactsRemoved">
      <arg name="pos" type="i" direction="out"/>
      <arg name="lenght" type="i" direction="out"/>
    </signal>
    <signal name="contactsCreated">
      <arg name="pos" type="i" direction="out"/>
      <arg name="lenght" type="i" direction="out"/>
    </signal>

    <method name="sort">
        <arg name="field" type="s" direction="in"/>
    </method>
    <method name="count">
        <arg type="i" direction="out"/>
    </method>
    <method name="contactsDetails">
        <arg name="fields" type="as" direction="in"/>
        <arg name="startIndex" type="i" direction="in"/>
        <arg name="pageSize" type="i" direction="in"/>
        <arg type="as" direction="out"/>
    </method>
    <method name="contactDetails">
        <arg name="fields" type="as" direction="in"/>
        <arg name="id" type="s" direction="in"/>
        <arg type="s" direction="out"/>
    </method>
</node>
